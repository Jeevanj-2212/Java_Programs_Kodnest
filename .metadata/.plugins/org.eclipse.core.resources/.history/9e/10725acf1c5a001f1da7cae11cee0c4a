package numberManipulation;

import java.util.Scanner;

public class PrimeNumberinrange {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count =0;
	
			
		
		for(int i=1;i<=n;i++ ) {
			if(n%i==0) {
				//System.out.println(i);
				count++;
			}
			if (count<=2) {
				System.out.println(i);
			}
		}
		

	}
	

}
